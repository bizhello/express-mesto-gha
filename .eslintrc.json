{
  "env": {
    "browser": true,
    "es2021": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "jsx": true
    }
  // "sourceType": "module"
  },
  "extends": "airbnb-base",
  "plugins": [
    "react"
  ],
  "rules": {
    "semi": ["error", "always"],
    "quotes": ["error", "double"]
  },
  "no-underscore-dangle": ["error", { "allow": ["foo_", "_bar"] }]
}